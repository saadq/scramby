    "raphael": "^2.2.7"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "blyss": "^7.0.0",
    "browserify": "^14.4.0",
    "flow-bin": "^0.49.1",
    "flow-typed": "^2.1.2",
